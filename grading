grading with if elsif
DECLARE
  marks NUMBER := 85;
BEGIN
  IF marks >= 90 THEN
     DBMS_OUTPUT.PUT_LINE('Grade: A');
  ELSIF marks >= 75 THEN
     DBMS_OUTPUT.PUT_LINE('Grade: B');
  ELSIF marks >= 60 THEN
     DBMS_OUTPUT.PUT_LINE('Grade: C');
  ELSE
     DBMS_OUTPUT.PUT_LINE('Grade: F');
  END IF;
END;


with switch case
DECLARE
  marks NUMBER := 85;
BEGIN
  CASE
    WHEN marks >= 90 THEN
       DBMS_OUTPUT.PUT_LINE('Grade: A');
    WHEN marks >= 75 THEN
       DBMS_OUTPUT.PUT_LINE('Grade: B');
    WHEN marks >= 60 THEN
       DBMS_OUTPUT.PUT_LINE('Grade: C');
    ELSE
       DBMS_OUTPUT.PUT_LINE('Grade: F');
  END CASE;
END;
